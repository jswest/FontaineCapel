<!doctype html>
%html
  %head
    %title Fontaine Capel
    %link{ :rel => "stylesheet", :href => "css/style.css", :style => "text/css" }
    %link{ :rel => "stylesheet", :href => "css/fonts.css", :style => "text/css" }
    %script{ :src => "js/jquery.js", :type => "text/javascript" }
    %script{ :src => "js/coffeescript.js", :type => "text/javascript" }
    %script{ :src => "js/initial_animation.js.coffee", :type => "text/coffeescript" }
    %script{ :src => "js/nav.js.coffee", :type => "text/coffeescript" }
    %script{ :src => "js/lightbox.js.coffee", :type => "text/coffeescript" }
  %body
    #lightbox-gloss
    #img-lightbox-wrapper
  
    #nav-and-header-control
    #nav-and-header
      %header#primary-header
        %nav#primary-nav
          %ul
            %li#installation{ "data-name" => "installation", "data-order" => 0, "data-num-images" => 16 } Installation
            %li#performance{ "data-name" => "performance", "data-order" => 1, "data-num-images" => 5 } Performance
            %li#studio_process{ "data-name" => "studio_process", "data-order" => 2, "data-num-images" => 10 } Studio // Process
            %li#three_dimensions{ "data-name" => "three_dimensions", "data-order" => 3, "data-num-images" => 6 } Three Dimensions
            %li#two_dimensions{ "data-name" => "two_dimensions", "data-order" => 4, "data-num-images" => 19 } Two Dimensions
        %hgroup
          %h1#primary-title Fontaine Capel
          %h2#primary-subtitle
    %section#content
